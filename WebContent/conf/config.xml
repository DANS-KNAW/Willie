<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<!-- decision engine -->
	
	<!-- distributed for live -->
	<!-- live -->
	<entry key="decision-engine">com.noterik.springfield.willie.queue.dist.DistributedDecisionEngine</entry>
    
	<!-- number of worker threads -->
	<entry key="number-of-workers">1</entry>
	
	<!-- default logging level -->
    <entry key="default-log-level">info</entry>
	
	<!-- ffmpeg path -->
    <entry key="ffmpeg-path"></entry>
	
	<!-- temporary directory -->
    <entry key="temporary-directory">/springfield/willie/tmp</entry>
    
    <!-- batch files path -->
    <entry key="batchFilesPath">/springfield/willie/batchfiles</entry>
    
    <!-- batch files extension -->
    <entry key="batchFilesExtension">.sh</entry>
</properties>